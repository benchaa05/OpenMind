                        CLIENTCOMPTBANC,
                        Mode=TwoWay,
                        NotifyOnValidationError=true,
                        ValidatesOnExceptions=true}" />
            </Grid>
            <Grid
                x:Name="grid8"
                Width="352"
                Margin="10,141,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                DataContext="{StaticResource cLIENTViewSource}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBox
                    x:Name="cLIENTNISTextBox"
                    Grid.Column="1"
                    Grid.Row="0"
                    Height="23"
                    Width="350"
                    Margin="0,3,3,3"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="{Binding
                        CLIENTNIS,
                        Mode=TwoWay,
                        NotifyOnValidationError=true,
                        ValidatesOnExceptions=true}" />
            </Grid>
            <Grid
                x:Name="grid9"
                Width="352"
                Margin="10,197,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                DataContext="{StaticResource cLIENTViewSource}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBox
                    x:Name="cLIENTNIFTextBox"
                    Grid.Column="1"
                    Grid.Row="0"
                    Height="23"
                    Width="350"
                    Margin="0,3,3,3"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="{Binding
                        CLIENTNIF,
                        Mode=TwoWay,
                        NotifyOnValidationError=true,
                        ValidatesOnExceptions=true}" />
            </Grid>
            <Label
                Margin="12,235,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="AI:" />
            <Grid
                x:Name="grid10"
                Width="350"
                Margin="10,254,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                DataContext="{StaticResource cLIENTViewSource}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBox
                    x:Name="cLIENTAITextBox"
                    Grid.Column="1"
                    Grid.Row="0"
                    Height="23"
                    Width="350"
                    Margin="0,3,3,3"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="{Binding
                        CLIENTAI,
                        Mode=TwoWay,
                        NotifyOnValidationError=true,
                        ValidatesOnExceptions=true}" />
            </Grid>
        </Grid>
        <Grid
            x:Name="grid15"
            Margin="245,167,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            DataContext="{StaticResource cLIENTViewSource}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
        </Grid>
        <Button
            x:Name="SaveBtn"
            Height="72"
            Width="72"
            Margin="0,0,-186,-265"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF009E00"
