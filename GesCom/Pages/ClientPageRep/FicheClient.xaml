            Width="500"
            Margin="10,258,0,-211"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            BorderThickness="1"
            CanUserResizeColumns="False"
            GridLinesVisibility="All"
            SelectedCellsChanged="ClientGrid_SelectedCellsChanged"
            SelectionMode="Single"
            VerticalGridLinesBrush="Gray" />

    </Grid>
</UserControl>