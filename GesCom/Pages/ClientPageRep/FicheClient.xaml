            <Image
                Height="72"
                Width="72"
                Margin="-17,-5,-12,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Source="../../image/appbar.add.png" />
        </Button>
        <Button
            x:Name="DeleteBtn"
            Height="72"
            Width="72"
            Margin="0,0,-371,-265"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FFB01A40"
            Click="Delete_Click">
            <Image
                Height="72"
                Width="72"
                Margin="-17,-5,-12,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Source="../../image/appbar.delete.png" />
        </Button>
        <Button
