        </Grid>
        <Label
            Margin="10,13,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Raison sociale:" />
        <Label
            Margin="10,65,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Nom:" />
        <Label
            Margin="10,120,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Prénom:" />
        <Label
            Margin="10,176,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Adresse:" />
        <Label
            Margin="326,13,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Logo:" />
        <Grid
            Height="288"
            Width="369"
            Margin="0,0,-379,-58"
            HorizontalAlignment="Right"
            VerticalAlignment="Top">
            <Label
                Margin="10,13,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Registre de commerce:" />
            <Label
                Margin="10,67,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="Compte Bancaire:" />
            <Label
                Margin="10,120,-99,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="NIS:" />
            <Label
                Margin="10,176,-99,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Content="NIF:" />
            <Grid
                x:Name="grid6"
                Width="352"
                Margin="10,34,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                DataContext="{StaticResource cLIENTViewSource}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBox
                    x:Name="cLIENTRCTextBox"
                    Grid.Column="1"
                    Grid.Row="0"
                    Height="23"
                    Width="350"
                    Margin="0,3,3,3"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Text="{Binding
                        CLIENTRC,
                        Mode=TwoWay,
                        NotifyOnValidationError=true,
                        ValidatesOnExceptions=true}" />
            </Grid>
            <Grid
                x:Name="grid7"
                Width="350"
                Margin="0,86,0,0"
                VerticalAlignment="Top"
                DataContext="{StaticResource cLIENTViewSource}">
                <Grid.ColumnDefinitions>
