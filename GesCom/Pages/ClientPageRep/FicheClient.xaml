            Height="36"
            Width="110"
            Margin="0,0,387,-261"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF613CBC"
            Click="FirstBtn_Click">
            <Image
                Height="36"
                Width="40"
                Margin="0,-5,-3,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="../../image/appbar.chevron.up.png" />
        </Button>
        <Button
            x:Name="PreviuesBtn"
            Height="36"
            Width="110"
            Margin="0,0,259,-261"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF613CBC"
            Click="previuesBtn_Click">
            <Image
                Height="36"
                Width="40"
                Margin="0,-5,-2,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="../../image/appbar.navigate.previous.png" />
        </Button>
        <Button
            x:Name="NextBtn"
            Height="36"
            Width="110"
            Margin="0,0,132,-261"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF613CBC"
            Click="NextBtn_Click">
            <Image
                Height="36"
                Width="40"
                Margin="0,-5,-1,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Source="../../image/appbar.navigate.next.png" />
        </Button>
        <Button
            x:Name="LastBtn"
            Height="36"
            Width="110"
            Margin="0,0,3,-261"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF613CBC"
            Click="LastBtn_Click">
            <Image
                Height="36"
                Width="40"
                Margin="0,-5,-3,-5"
