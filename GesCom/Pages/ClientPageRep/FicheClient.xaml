<UserControl
    x:Class="GesCom.Pages.ClientPageRep.FicheClient"
    x:Name="FicheClientPg"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:DataBaseProject="clr-namespace:DataBaseProject;assembly=DataBaseProject"
  
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
  
    Height="540.375"
    Width="900"
    Loaded="UserControl_Loaded"
    mc:Ignorable="d">
    <UserControl.Resources>
        <CollectionViewSource
            x:Key="cLIENTViewSource"
            d:DesignSource="{d:DesignInstance
                {x:Type DataBaseProject:CLIENT},
                CreateList=True}" />
        <CollectionViewSource
            x:Key="cLIENTTELEPHONEsViewSource"
            Source="{Binding
                TELEPHONEs,
                Source={StaticResource cLIENTViewSource}}" />
        <CollectionViewSource
            x:Key="tELEPHONEViewSource"
            d:DesignSource="{d:DesignInstance
                {x:Type DataBaseProject:TELEPHONE},
                CreateList=True}" />
    </UserControl.Resources>
    <Grid
        Width="513"
        Margin="0,28,0,276"
        HorizontalAlignment="Left"
        DataContext="{StaticResource cLIENTTELEPHONEsViewSource}"
        Style="{StaticResource ContentRoot}">
        <Grid
            x:Name="grid1"
            Width="200"
            Margin="10,86,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            DataContext="{StaticResource cLIENTViewSource}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <TextBox
                x:Name="cLIENTLASTNAMETextBox"
                Grid.ColumnSpan="2"
                Grid.Row="0"
                Height="23"
                Width="200"
                Margin="0,3,-92,3"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Text="{Binding
                    CLIENTLASTNAME,
                    Mode=TwoWay,
                    NotifyOnValidationError=true,
                    ValidatesOnExceptions=true}" />
        </Grid>
        <Grid
            x:Name="grid2"
            Width="200"
            Margin="10,141,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            DataContext="{StaticResource cLIENTViewSource}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <TextBox
                x:Name="cLIENTFIRSTNAMETextBox"
                Grid.Column="1"
                Grid.Row="0"
                Height="23"
                Width="200"
                Margin="0,3,-76,3"
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                Text="{Binding
                    CLIENTFIRSTNAME,
                    Mode=TwoWay,
                    NotifyOnValidationError=true,
                    ValidatesOnExceptions=true}" />
        </Grid>
        <Grid
            x:Name="grid3"
            Width="500"
            Margin="10,197,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            DataContext="{StaticResource cLIENTViewSource}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="Auto"
                    MinWidth="48" />
                <ColumnDefinition
                    Width="Auto"
                    MinWidth="244" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <TextBox
                x:Name="cLIENTADRESSTextBox"
