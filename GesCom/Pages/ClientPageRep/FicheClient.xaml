            <Image
                Height="72"
                Width="72"
                Margin="-17,-5,-12,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Source="../../image/appbar.save.png" />
        </Button>
        <Button
            x:Name="UpdateBtn_Copy"
            Height="72"
            Width="72"
            Margin="0,0,-278,-265"
            HorizontalAlignment="Right"
            VerticalAlignment="Bottom"
            Background="#FF2778EC"
            Click="UpdateBtn_Click">
            <Image
                Height="72"
                Width="72"
                Margin="-17,-5,-12,-5"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Source="../../image/appbar.arrow.up.down.png" />
        </Button>
        <Button
